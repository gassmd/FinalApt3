<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="383.0" prefWidth="485.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View_Controller.CustomerController">
    <children>
        <Button layoutX="49.0" layoutY="235.0" mnemonicParsing="false" onAction="#deleteCustomerBtn" prefHeight="69.0" prefWidth="138.0" text="Delete">
            <font>
                <Font name="System Bold" size="30.0" />
            </font></Button>
        <Button layoutX="49.0" layoutY="166.0" mnemonicParsing="false" onAction="#updateCustomerBtn" prefHeight="69.0" prefWidth="138.0" text="Update">
            <font>
                <Font name="System Bold" size="28.0" />
            </font></Button>
        <Button layoutX="49.0" layoutY="97.0" mnemonicParsing="false" onAction="#addCustomerBtn" prefHeight="69.0" prefWidth="138.0" text="Add">
            <font>
                <Font name="System Bold" size="30.0" />
            </font></Button>
        <TableView fx:id="customerTable" layoutX="192.0" layoutY="92.0" prefHeight="217.0" prefWidth="391.0">
            <columns>
                <TableColumn fx:id="customerNameCol" prefWidth="137.0" text="custName" />
                <TableColumn fx:id="addressIdCol" prefWidth="124.0" text="address" />
            <TableColumn fx:id="phoneCol" minWidth="7.0" prefWidth="129.0" text="phone" />
            </columns>
        </TableView>
        <Label layoutX="35.0" layoutY="14.0" prefHeight="69.0" prefWidth="205.0" text="Customer Records">
            <font>
                <Font name="Bell MT Bold" size="24.0" />
            </font>
        </Label>
        <Button layoutX="537.0" layoutY="14.0" mnemonicParsing="false" onAction="#backBtnCustomer" text="Back" />
      <Button fx:id="refreshBtn" layoutX="530.0" layoutY="49.0" mnemonicParsing="false" onAction="#refreshBtn" text="Refresh" />
      <Button layoutX="275.0" layoutY="309.0" mnemonicParsing="false" onAction="#showCustomerRecord" prefHeight="33.0" prefWidth="226.0" text="View Selected Customer Record">
         <font>
            <Font name="System Bold Italic" size="14.0" />
         </font>
      </Button>
    </children>
</AnchorPane>
