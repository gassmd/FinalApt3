            <Font name="Arial Black" size="14.0" />
         </font>
      </Button>
      <Button layoutX="446.0" layoutY="313.0" mnemonicParsing="false" onAction="#apptDetailsBtn" prefHeight="11.0" prefWidth="214.0" text="Appointment Details">
